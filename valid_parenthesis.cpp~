#include <stack>

class Solution {
public:
  bool isValid(std::string s) {
    int count=0;
    std::stack<string> paren;
    for(string a : s) {
      if(a == '(' or
	 a == '[' or
	 a == '{') {
	paren.push(a);
	count+=1;
      }

      if(a==')' and paren.top()=='(')
	count-=1;
      if(a=='}' and paren.top()=='{')
	count-=1;
      if(a==']' and paren.top()=='[')
	count-=1;
    }

    return (count==0)?true:false;
  }

};

int main(int argc, char **argv[])
{
  Solution sol = Solution();
  std::cout<<sol.isValid("{}{}{}{}[]")<<std::endl;
}
